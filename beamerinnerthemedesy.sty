\ProvidesPackage{beamerinnerthemedesy}[2017/01/20]

\mode<presentation>

\defbeamertemplate{title page}{desy}{
	\vspace{-0.7cm}
	\usebeamerfont{title}
	\begin{beamercolorbox}[wd=\paperwidth,ht=1.74cm,dp=1ex,leftskip=0.3cm,rightskip=0.3cm]{title}
		\inserttitle\usebeamercolor[fg]{local structure}.\par%
	\end{beamercolorbox}
	\usebeamerfont{subtitle}
	\begin{beamercolorbox}[wd=\paperwidth,ht=2.8ex,dp=1ex,leftskip=0.3cm,rightskip=0.3cm]{subtitle}
		\insertsubtitle\par%
	\end{beamercolorbox}
	\begin{beamercolorbox}[wd=\paperwidth,ht=3.2cm,sep=0cm]{titlegraphic}
		\usebeamertemplate{titlegraphic}
	\end{beamercolorbox}
	\usebeamerfont{text in title}
	\vspace{1ex}
	\hfill
	\begin{beamercolorbox}[wd=0.4\paperwidth,left]{text in title}
		\usebeamerfont*{author}\usebeamercolor*[fg]{author}\insertauthor\par
		\usebeamerfont*{institute}\usebeamercolor*[fg]{institute}\insertinstitute\par
		\usebeamerfont*{date}\usebeamercolor*[fg]{date}\insertdate
	\end{beamercolorbox}
	\vfill
	\begin{beamercolorbox}[wd=\paperwidth,ht=1.9cm,sep=0cm]{logo in title}
		\usebeamertemplate{logo in title}
	\end{beamercolorbox}
	\vspace{-1.1cm}
}

\defbeamertemplate{titlegraphic}{user}{
	\inserttitlegraphic
}

\defbeamertemplate{titlegraphic}{desy}{
	\includegraphics[width=\paperwidth]{\string./titlegraphics/desy.jpg}
}

\defbeamertemplate{titlegraphic}{cms}{
	\includegraphics[width=\paperwidth]{\string./titlegraphics/cms.jpg}
}

\defbeamertemplate{logo in title}{desy}{
	\hspace{0.5cm}
	\usebeamertemplate{helmholtz logo in title}
	\hfill
	\usebeamertemplate{other logo in title}
	\hspace{0.5cm}
	\vspace{0.5cm}
}

\defbeamertemplate{helmholtz logo in title}{en}{
	\includegraphics[height=0.8cm]{\string./logos/helmholtz_en}
}

\defbeamertemplate{helmholtz logo in title}{de}{
	\includegraphics[height=0.8cm]{\string./logos/helmholtz_de}
}

\defbeamertemplateparent{helmholtz logo}{helmholtz logo in title}{}

\defbeamertemplate{other logo in title}{desy}{
	\includegraphics[height=1.4cm]{\string./logos/desy}
}

\defbeamertemplate{other logo in title}{desycms}{
	\includegraphics[height=1.4cm]{\string./logos/desy}
	\hspace{0.5cm}
	\includegraphics[height=1.4cm]{\string./logos/cms}
}

\defbeamertemplate{other logo in title}{desycmsbril}{
	\includegraphics[height=1.4cm]{\string./logos/desy}
	\hspace{0.5cm}
	\includegraphics[height=1.4cm]{\string./logos/cms}
	\hspace{0.5cm}
	\includegraphics[height=1.4cm]{\string./logos/bril}
}

